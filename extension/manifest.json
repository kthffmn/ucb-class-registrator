{
  "name": "UCB Sketch Class",
  "manifest_version": 1,
  "description": "Clicks 'Register' on a UCB class page",
  "version": "1.0",
  "content_scripts": [
    {
      "matches":["http://newyork.ucbtrainingcenter.com/courses/view/*"],
      "js": ["functions.js", "click-register.js"]
    },
    {
      "matches":["http://newyork.ucbtrainingcenter.com/courses/policies/*"],
      "js": ["functions.js", "click-agree.js"]
    },
    {
      "matches":["https://secure.ucbtheatre.com/courses/register/*"],
      "js": ["functions.js", "fill-in-form.js"]
    }
  ], "permissions": [
  	"tabs", "http://newyork.ucbtrainingcenter.com/courses/view/*"
  ]
}